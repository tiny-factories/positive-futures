// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Story {
  id        String    @id @default(uuid())
  date      String?
  scenario  String?
  title     String?
  content   String?
  location  String?
  createdAt DateTime? @default(now())
}

model Poem {
  id        String    @id @default(uuid())
  scenario  String?
  content   String?
  createdAt DateTime? @default(now())
}


