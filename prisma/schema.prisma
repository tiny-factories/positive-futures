// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Story {
  id         String    @id
  location   String?
  occupation String?
  date       String?
  scenario   String?
  content    String?
  createdAt  DateTime? @default(now())
}

